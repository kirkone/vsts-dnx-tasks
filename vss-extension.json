{
    "manifestVersion": 1,
    "id": "vsts-dnx-tasks",
    "name": "VSTS DNX Tasks",
    "version": "0.1.10",
    "publisher": "kirkone",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "scope": [
        "vso.build",
        "vso.serviceendpoint"
    ],
    "description": "Build and Release Management extensions that enable you to package and publish projects using the dotnet cli.",
    "categories": [
        "Build and release"
    ],
    "tags": [
        "ASP.NET Core",
        "dotnet",
        "Build",
        "Publish",
        "Package"
    ],
    "screenshots": [
        {
            "path": "Extension/Images/Screenshots/Screenshot1.png"
        },
        {
            "path": "Extension/Images/Screenshots/Screenshot2.png"
        }
    ],
    "content": {
        "details": {
            "path": "readme.md"
        },
        "license": {
            "path": "license.txt"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/kirkone/vsts-dnx-tasks"
        },
        "support": {
            "uri": "https://github.com/kirkone/vsts-dnx-tasks/issues"
        }
    },
    "branding": {
        "color": "rgb(27, 27, 27)",
        "theme": "dark"
    },
    "icons": {
        "default": "Extension/Images/ExtensionIcon.png"
    },
    "files": [
        {
            "path": "VSTS.DNX.Tasks.BuildWebPackage/BuildWebPackage.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildWebPackage/Common.psm1",
            "contentType": "text/plain"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildWebPackage/InstallDotnet.psm1",
            "contentType": "text/plain"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildWebPackage/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildWebPackage/icon.png"
        },

        {
            "path": "VSTS.DNX.Tasks.BuildNugetPackage/BuildNugetPackage.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildNugetPackage/Common.psm1",
            "contentType": "text/plain"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildNugetPackage/InstallDotnet.psm1",
            "contentType": "text/plain"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildNugetPackage/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.BuildNugetPackage/icon.png"
        },

        {
            "path": "VSTS.DNX.Tasks.PublishWebPackage/PublishWebPackage.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.PublishWebPackage/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.PublishWebPackage/icon.png"
        },

        {
            "path": "VSTS.DNX.Tasks.SlotSwap/SwapDepoymentSlots.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.SlotSwap/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.SlotSwap/icon.png"
        },

        {
            "path": "VSTS.DNX.Tasks.ClearNugetCache/ClearNugetCache.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.ClearNugetCache/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.ClearNugetCache/icon.png"
        },

        {
            "path": "VSTS.DNX.Tasks.GenerateChangeLog/GenerateChangeLog.ps1"
        },
        {
            "path": "VSTS.DNX.Tasks.GenerateChangeLog/task.json"
        },
        {
            "path": "VSTS.DNX.Tasks.GenerateChangeLog/icon.png"
        }
    ],
    "contributions": [
        {
            "id": "VSTS.DNX.Tasks.BuildWebPackage",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.BuildWebPackage"
            }
        },

        {
            "id": "VSTS.DNX.Tasks.BuildNugetPackage",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.BuildNugetPackage"
            }
        },

        {
            "id": "VSTS.DNX.Tasks.PublishWebPackage",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.PublishWebPackage"
            }
        },

        {
            "id": "VSTS.DNX.Tasks.SlotSwap",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.SlotSwap"
            }
        },

        {
            "id": "VSTS.DNX.Tasks.ClearNugetCache",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.ClearNugetCache"
            }
        },

        {
            "id": "VSTS.DNX.Tasks.GenerateChangeLog",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "VSTS.DNX.Tasks.GenerateChangeLog"
            }
        }
    ]
}
